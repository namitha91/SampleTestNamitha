package com.amazon;

import java.util.LinkedList;
import java.util.Queue;

class Node{
	int d;
	Node next;
	Node(int data) {
		d=data;
		next=null;
	}
}
public class RotateAntiClock {
	
	public static void  main(String[] args) {
		Queue<Integer> l=new LinkedList<Integer>();
		l.add(10);
		l.add(20);
		l.add(30);
		l.add(40);
		l.add(50);
		l.add(60);
		
		rotate(l.peek(),4);
		
	}

