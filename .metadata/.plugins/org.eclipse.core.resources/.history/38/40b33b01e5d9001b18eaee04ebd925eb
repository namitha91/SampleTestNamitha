package com.walmart;

public class ArrayIndexAndNumber {
	
	
	public static void main(String[] args) {
		int arr[]= {-1,-1,6,1,9,3,2,-1,4,-1};
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr.length;j++) {
				if(i==arr[j]) {
					arr[i]=i;
				}else
					arr[i]=-1;
			}
		}
		System.out.println(arr);
	}

}
